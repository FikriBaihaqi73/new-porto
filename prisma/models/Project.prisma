model Project {
  id          String   @id @default(uuid())
  title       String
  description String
  image       String
  type        String
  status      String
  progress    Int
  techStack   Json
  liveUrl     String?
  githubUrl   String?
  featured    Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
